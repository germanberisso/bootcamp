//primera parte//
public class Main{
  public static void main(Stirng[] args){
    int a=1;
    int b=2;
    int c=3;
    suma(a,b,c);
  }
  public static int suma(int a,int b,int c){
  return suma=a+b+c;
  }
}
//segunda parte//
public class Coche{
  private int puertas=0;
  public void agregarPuerta(Coche c){
    c.puertas++;
  }
  public static void main(Stirng[] args){
    Coche c=new Coche;
    agregarPuerta(c);
    System.out.println(c.puertas);
  }
